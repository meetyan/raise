{
	"name": "Raise",
	"description": "A simple (and unofficial) GitHub Trending client that lives in your menubar.",
	"version": "1.1.0",
	"manifest_version": 3,
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "/chrome/16.png",
			"32": "/chrome/32.png",
			"48": "/chrome/48.png",
			"128": "/chrome/128.png"
		}
	},
	"icons": {
		"16": "/chrome/16.png",
		"32": "/chrome/32.png",
		"48": "/chrome/48.png",
		"128": "/chrome/128.png"
	}
}